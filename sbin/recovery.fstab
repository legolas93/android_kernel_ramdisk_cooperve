# mount     fstype  device                  [device2]   [fstype2]   [fsoptions]         [fsoptions2]

/system     rfs     /dev/block/stl9           NULL        NULL         NULL               check=no
/cache      rfs     /dev/block/stl10          NULL        NULL         NULL               check=no
/data       rfs     /dev/block/stl11          NULL        NULL         NULL               check=no
/sdcard     vfat    /dev/block/mmcblk0p1      NULL        NULL         NULL               NULL
/sd-ext     ext3    /dev/block/mmcblk0p2      NULL        NULL         NULL               NULL
